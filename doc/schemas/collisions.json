{
	"$schema": "https://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"Width": {
			"type": "integer",
			"minimum": 1
		},
		"Height": {
			"type": "integer",
			"minimum": 1
		},
		"Map": {
			"type": "string",
			"pattern": "^(\\.|\\X)+(\\/(\\.|\\X)+)*$"
		}
	},
	"additionalProperties": false,
	"required": [ "Width", "Height" ]
}